		<script type="text/javascript">
	$(function() {
		// 영화관 지역 변경
		$('#theater').on('change', function() {
			if($(this).val() != '') {
				fn_searchTheaterList($(this).val());
			} else {
				fn_resetTheater();
			}
		});

		// 상단 탭 & 검색 클릭
		$('.tabBtn').on('click', function() {
			var no = $(this).find('button').data('no');

			// 탭클릭시 검색조건 초기화
			$('#theater').val('').change();
			$('#theater02').val('').change();
			$('#searchTxt').val('');
			$('[name=currentPage]').val(1);

			if(no != undefined && no != '') {
				fn_selectIrList($(this).find('button').data('no'));
			} else {
				fn_selectIrList();
			}

			if(no == 81) {
				$('#theater, #theater02').selectpicker('hide');
			} else {
				$('#theater, #theater02').selectpicker('show');
			}
		});

		// 검색 클릭
		$('#searchBtn').on('click', function() {
			if ($('#theater').val() != '' && $('#theater02').val() == '') {
				gfn_alertMsgBox('극장을 선택해 주세요.');
				return;
			}

			// 검색시 전체검색
			//if($('#searchTxt').val() != '') {
				$('.tabBtn.on').removeClass('on');
				$('.tabBtn').eq(0).addClass('on');

				$('[name=currentPage]').val(1);
			//}

			fn_selectIrList($('.tabBtn.on').find('button').data('no'));
		});

		// 검색 엔터키
		$('#searchTxt').on('keydown', function(e) {
			if(e.keyCode == 13) {
				e.preventDefault();
				$('#searchBtn').click();
			}
		});

		fn_selectIrList($('.tabBtn.on').find('button').data('no'));

		MegaboxUtil.Common.bindSelectBox(); // 셀렉트 박스 라이브러리 적용
	});

	// 영화관 코드 목록 초기화 함수
	function fn_resetTheater() {
		$('#theater02 option').remove();
		$('#theater02').append($('<option>').val('').text('극장 선택'));
		$('#theater02').attr('disabled', true);

		MegaboxUtil.Common.refreshSelectBox($('#theater02')); // 셀렉트박스 데이터 초기화
	}

	// 영화관 코드 목록 조회 함수
	function fn_searchTheaterList(cd) {
		$.ajaxMegaBox({
		 	url: '/on/oh/ohj/Company/theaterCdList.do',
		 	data: JSON.stringify({ areaCd: cd }),
		 	success: function(result) {
		 		var items = result.theaterCdList;
		 		var target = $('#theater02');
		 		var option;

		 		fn_resetTheater();

		 		for(var i = 0; i < items.length; ++i) {
		 			option = $('<option>').val(items[i].brchNo).html(items[i].brchNm);

		 			target.append(option);
		 		}

		 		target.attr('disabled', false);

		 		MegaboxUtil.Common.refreshSelectBox($('#theater02'));
		  	}
		});
	}

	// 게시글 목록 조회
	function fn_selectIrList(cateNo) {
		var currentPage = $('[name=currentPage]').val() ? $('[name=currentPage]').val() : 1;
		var cate2No = cateNo && cateNo == '81' ? '' : $('#theater02').val();

		var params = {
			cateNo: cateNo ? cateNo : '',
			cate2No: cate2No,
			searchTxt: $('#searchTxt').val()
		};

		var options = {};
		options.url = '/on/oh/ohi/CustCen/customerNoticeList.do';
		options.paramData = params;
		options.currentPage = currentPage;

		var noticeOption = {type : {tag : 'span', css : 'font-green'}, regTxt : $('#searchTxt').val()};

		/* 성공시 처리해야 할부분 */
		options.successCallBack = function(data) {
			var totCount  = Number(data.totCount);
			var target = $('div.table-wrap > table.board-list > tbody');

			target.find('tr').remove();

			$('.result-count').html('<strong>전체 ' + '<em class="font-gblue">' + Number(totCount).format() + '</em>' + '건</strong>');

			if(data.boardList.length > 0) {
				var tr, td1, td2, td3, td4, th, a;

				target.find('tr').remove();

				$.each(data.boardList, function(i, v) {
					tr = $('<tr>');
					a = $('<a>').attr({ 'href': '#', 'class': 'moveBtn', 'data-no': v.artiNo, 'title': '공지사항 상세보기' }).html(gfn_getTighlight(noticeOption, v.artiTitle));
					td1 = $('<td>').text(v.rowNum);
					td2 = $('<td>').html(v.cateNo1 == 81 ? v.cateNm : v.cate2Nm);
					td3 = $('<td>').text(v.cate3Nm);
					td4 = $('<td>').text(v.fstRegDtStr);
					th = $('<th>').append(a);

					tr.append(td1).append(td2).append(td3).append(th).append(td4);
					target.append(tr);
				});

				fn_bindMoveEvent();
			} else {
				tr = $('<tr>');
				td1 = $('<td>').attr('colspan', 5).text('목록이 없습니다.');

				tr.append(td1);
				target.append(tr);
			}
		};

		gfn_setPage(options);
	}

	function fn_bindMoveEvent() {
		$('.moveBtn').off();
		$('.moveBtn').on('click', function(e) {
			e.preventDefault();

			//$('[name=artiNo]').val($(this).data('no'));
			$('[name=cateNo]').val($('.tabBtn.on').find('button').data('no'));
			$('[name=theater]').val($('#theater').val());
			$('[name=theater02]').val($('#theater02').val());
			$('[name=cate2No]').val($('#theater02').val());
			$('[name=searchTxt]').val($('#searchTxt').val());
			$('[name=currentPage]').val($('.pagination .active').text());

			var artiNo = $(this).data('no');

			var form = $(document.forms.moveFrm);
			form.attr('action', '/support/notice/detail?artiNo='+artiNo);
			form.submit();
		});
	}
</script>



